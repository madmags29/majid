{
    "version": 2,
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/.next",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "backend/api/index.ts"
        },
        {
            "source": "/admin/(.*)",
            "destination": "admin/$1"
        },
        {
            "source": "/AgodaPartnerVerification.htm",
            "destination": "frontend/public/AgodaPartnerVerification.htm"
        }
    ]
}